{
  "settings": {
    "launchDelaySeconds": 30,
    "autoStartOnBoot": false,
    "listenerHeadless": false,
    "autoLoginHeadless": false
  },
  "databasePath": "./data.sqlite",
  "pages": [
    {
      "id": "system-a",
      "name": "系统A",
      "url": "https://example.com/a/dashboard",
      "refreshSeconds": 30,
      "manualLogin": false,
      "listeners": [
        {
          "name": "授权Token",
          "urlPatterns": ["https://example.com/a/api/*"],
          "headerName": "authorization",
          "primaryKey": "system_a_token"
        }
      ],
      "autoLogin": {
        "enabled": true,
        "hasLoginTypeButton": true,
        "loginTypeSelector": "#login-type",
        "usernameSelector": "#username",
        "username": "demo",
        "passwordSelector": "#password",
        "password": "demo123",
        "loginButtonSelector": "#login-btn",
        "postLoginUrl": "https://example.com/a/dashboard"
      }
    }
  ]
}
